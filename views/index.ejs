<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    
    
    <span>
        Retrieve I = <span id="i"><%= i %></span> Ampere
    </span><br/>
    <span>
        Retrieve R = <span id="r"><%= r %></span> Ohm
    </span><br/>
    
    <span>
        Predicted V = <span id="v">**</span> Volt
    </span><br/>
    <span>
        Predicted P = <span id="p">**</span> Watt
    </span><br/>
    <span>
        Labels = <span id="l">**</span>
    </span>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    
    <script>	
	function classify(){
	  let i = $("#i").text();
	  let r = $("#r").text();
	  
          $.get(`/api/classify/${i}/${r}`).done(function(dt){
              
              $("#v").text(dt['jres'][0])
              $("#p").text(dt['jres'][1])
              $("#l").text(dt['jres_'])

              console.log(dt['jres'])
              console.log(dt['jres_'])
          });
	}
	
	classify();
	
        setInterval(function(){ 
        	classify();
        }, 5000);

    </script>
  

</body>
</html>
